{
  "name": "agile-git",
  "productName": "agile-git",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "bundled/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently \"npm run bundle\" \"grunt watch\" \"npm start\"",
    "bundle": "npx babel --watch src --out-dir bundled",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Nduduzo Shabalala",
    "email": "ndshabal021@student.wethinkcode.co.za"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "agile_git"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.41",
    "babel-preset-react-app": "^3.1.2",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "electron": "20.1.0",
    "grunt": "~0.4.5",
    "grunt-concat-css": "^0.3.2",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "~0.5.0",
    "grunt-contrib-watch": "^1.1.0"
  }
}
